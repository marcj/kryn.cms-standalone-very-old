        "marijnh/codemirror": ">=v3.1",
        "sybio/image-workshop": "dev-master",
        "ckeditor/ckeditor": "*",
        "propel/propel": "dev-master",
        "composer/composer": "*",
        "krynlabs/kryn-publication-bundle": "*",
